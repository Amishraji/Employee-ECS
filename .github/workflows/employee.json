{
    "containerDefinitions": [
        {
            "name": "employee-container",
            "image": "992382462614.dkr.ecr.us-west-2.amazonaws.com",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "environment": [
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "{{arn:aws:ssm:us-west-2:992382462614:parameter/demoapp/access_key_id/20240410/us-west-2/alias/aws/ssm/aws4_request}}"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "{{arn:aws:ssm:us-west-2:992382462614:parameter/demoapp/secret_access_key/20240410/us-west-2/alias/aws/ssm/aws4_request}}"
        }],
            "essential": true,
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/employee-family",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "employee-family",
    "executionRoleArn": "arn:aws:iam::992382462614:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": [{
        "key": "employee",
        "value": "Latest"
    }]
}
